<?xml version="1.0" encoding="UTF-8"?> 
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" 
	version="1.0"> 
	
     <mapped-superclass class="es.mde.acing.utils.MaterialL" access="FIELD">
 		<attributes>
        
		<basic name="descripcion">
		    <column name="descripcion"/>
		</basic>
		
		<basic name="dimensiones">
		    <column name="dimensiones"/>
		</basic>
		
		<basic name="peso">
		    <column name="peso"/>
		</basic>
		
		<basic name="estado">
		    <column name="estado"/>
		</basic>
		
		<basic name="fechaAdquisicion">
		    <column name="fechaAdquisicion"/>
		</basic>
		
		<basic name="fechaOferta">
		    <column name="fechaOferta"/>
		</basic>
		
		<basic name="imagen">
		    <column name="imagen"/>
		</basic>
		
		<basic name="milis">
		    <column name="milis"/>
		</basic>
		
		<basic name="cantidad">
		    <column name="cantidad"/>
		</basic>
            
            <many-to-one name="dptoOferta" fetch="LAZY"  optional="false" >
                <join-column name="dptoO_id"/>
            </many-to-one>
            
            <many-to-one name="dptoAdquisicion" fetch="LAZY" >
                <join-column name="dptoA_id"/>
            </many-to-one>
            
            <many-to-one name="categoria" fetch="LAZY"  optional="false" >
                <join-column name="categoria_id" />
            </many-to-one>
            
        </attributes>

    </mapped-superclass>

</entity-mappings>