<?xml version="1.0" encoding="UTF-8"?> 
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" 
	version="1.0"> 
	
     <mapped-superclass class="es.mde.acing.utils.DepartamentoL" access="FIELD">

        <attributes>
		<basic name="nombre" optional="false">
		    <column name="nombre" unique="true"/>
		</basic>
		<basic name="abreviatura" optional="false">
		    <column name="abreviatura" unique="true"/>
		</basic>
		<basic name="acuartelamiento" optional="false">
		    <column name="acuartelamiento"/>
		</basic>
		<basic name="email" optional="false">
		    <column name="email"/>
		</basic>
		<basic name="credito" optional="false">
		    <column name="credito"/>
		</basic>
		<basic name="responsableEmpleo" optional="false">
		    <column name="responsableEmpleo"/>
		</basic>
		<basic name="responsableNombre" optional="false">
		    <column name="responsableNombre"/>
		</basic>
		<basic name="telefono" optional="false">
		    <column name="telefono"/>
		</basic>
		<basic name="direccion" optional="false">
		    <column name="direccion"/>
		</basic>
		<basic name="latitud" optional="false">
		    <column name="latitud"/>
		</basic>
		<basic name="longitud" optional="false">
		    <column name="longitud"/>
		</basic>
  			<one-to-many name="materialesOfertados" mapped-by="materialesOfertados" target-entity="es.mdef.apitruequet.entidades.Material"/>
            <one-to-many name="materialesAdquiridos" mapped-by="materialesAdquiridos" target-entity="es.mdef.apitruequet.entidades.Material"/>

        </attributes>
    </mapped-superclass>

</entity-mappings>